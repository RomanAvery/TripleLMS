{
    "name": "plus-education/pluslms",
    "type": "project",
    "description": "Plus Education is a Learning Management System, I believe that course development should be fun and creative. Plus Education eliminates the hassle of creating online courses, through an intuitive and consistent interface.",
    "keywords": [
        "LMS",
        "Education",
        "Learning Management System",
        "E-learning",
        "Laravel",
        "Laravel Nova",
        "PHP"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.4",
        "barryvdh/laravel-dompdf": "^1.0.0",
        "eminiarts/nova-tabs": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "0.5.4",
        "laravel/framework": "^8.16.1",
        "laravel/jetstream": "^2.7",
        "laravel/nova": "^4.0",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.0",
        "lms/activity-comments": "*",
        "lms/activity-scores": "*",
        "lms/course-gradebook": "*",
        "lms/files": "*",
        "lms/gradebook": "*",
        "lms/link-file": "*",
        "lms/notices": "*",
        "lms/teacher-topic-comment": "*",
        "lms/users-types": "*",
        "maatwebsite/excel": "^3.1.40",
        "monaye/simple-link-button": "^1.0",
        "phoenix-lib/nova-nested-tree-attach-many": "^1.7",
        "pixelcreation/nova-field-sortable": "^3.0.0",
        "pusher/pusher-php-server": "^6.1",
        "sentry/sentry-laravel": "^2.12",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft": "^4.1",
        "spatie/eloquent-sortable": "^4.0.1",
        "spatie/nova-backup-tool": "^5.0.1",
        "spatie/nova-tags-field": "^4.0.3",
        "tightenco/ziggy": "^1.4",
        "vyuldashev/nova-permission": "^3.0.1",
        "whitecube/nova-flexible-content": "^1.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/ActivityScores"
        },
        {
            "type": "path",
            "url": "./nova-components/ActivityComments"
        },
        {
            "type": "path",
            "url": "./nova-components/UsersTypes"
        },
        {
            "type": "path",
            "url": "./nova-components/Files"
        },
        {
            "type": "path",
            "url": "./nova-components/LinkFile"
        },
        {
            "type": "path",
            "url": "./nova-components/TeacherTopicComment"
        },
        {
            "type": "path",
            "url": "./nova-components/Gradebook"
        },
        {
            "type": "path",
            "url": "./nova-components/Notices"
        },
        {
            "type": "path",
            "url": "./nova-components/CourseGradebook"
        }
    ],
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "NovaModules\\": "nova-modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
