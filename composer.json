{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "chris-ware/nova-breadcrumbs": "^1.3",
        "digital-creative/nova-filepond": "^0.2.13",
        "digital-creative/nova-inline-morph-to": "^0.2.1",
        "eminiarts/nova-tabs": "^1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.2.11",
        "khalin/nova-link-field": "^1.2",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.1",
        "laravel/nova": "~3.9.2",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "michielkempen/nova-order-field": "^2.0",
        "spatie/nova-backup-tool": "^4.0",
        "spatie/nova-tags-field": "^3.0",
        "vyuldashev/nova-permission": "^2.9",
        "whitecube/nova-flexible-content": "^0.2.5"
    },

    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],

    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
